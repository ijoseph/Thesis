@article{campisi_senescent_2005,
  title = {Senescent {{Cells}}, {{Tumor Suppression}}, and {{Organismal Aging}}: {{Good Citizens}}, {{Bad Neighbors}}},
  volume = {120},
  issn = {0092-8674},
  url = {http://www.sciencedirect.com/science/article/pii/S009286740500111X},
  doi = {10.1016/j.cell.2005.02.003},
  shorttitle = {Senescent {{Cells}}, {{Tumor Suppression}}, and {{Organismal Aging}}},
  abstract = {Cells from organisms with renewable tissues can permanently withdraw from the cell cycle in response to diverse stress, including dysfunctional telomeres, DNA damage, strong mitogenic signals, and disrupted chromatin. This response, termed cellular senescence, is controlled by the p53 and RB tumor suppressor proteins and constitutes a potent anticancer mechanism. Nonetheless, senescent cells acquire phenotypic changes that may contribute to aging and certain age-related diseases, including late-life cancer. Thus, the senescence response may be antagonistically pleiotropic, promoting early-life survival by curtailing the development of cancer but eventually limiting longevity as dysfunctional senescent cells accumulate.},
  timestamp = {2014-01-29T22:33:18Z},
  number = {4},
  journaltitle = {Cell},
  shortjournal = {Cell},
  author = {Campisi, Judith},
  urldate = {2014-01-29},
