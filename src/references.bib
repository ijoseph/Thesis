  urldate = {2016-03-17},
  date = {2004-11-30},
  pages = {16837--16842},
  keywords = {cis-regulatory elements,DNase I-hypersensitive sites,gene regulation},
  file = {Full Text PDF:/Users/ijoseph/Library/Application Support/Zotero/Profiles/gtzxqh9t.default/zotero/storage/X5F5NQCF/Sabo et al. - 2004 - Discovery of functional noncoding elements by digi.pdf:application/pdf;Snapshot:/Users/ijoseph/Library/Application Support/Zotero/Profiles/gtzxqh9t.default/zotero/storage/CRWXKICV/16837.html:text/html},
