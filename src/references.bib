  author = {Dudoit, Sandrine and Fridlyand, Jane and Speed, Terence P},
  urldate = {2013-10-30},
  date = {2002},
  pages = {77--87},
  file = {Full Text PDF:/Users/ijoseph/Library/Application Support/Zotero/Profiles/gtzxqh9t.default/zotero/storage/3ERI9WPB/Dudoit et al. - 2002 - Comparison of Discrimination Methods for the Class.pdf:application/pdf;Snapshot:/Users/ijoseph/Library/Application Support/Zotero/Profiles/gtzxqh9t.default/zotero/storage/27BJ5G3W/016214502753479248.html:text/html}
