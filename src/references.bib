  urldate = {2014-10-17},
  date = {2013-04-23},
  pages = {6789--6794},
  keywords = {allele-specific binding,differential binding,histone modification,next-generation sequencing,RNA-seq},
  file = {Full Text PDF:/Users/ijoseph/Library/Application Support/Zotero/Profiles/gtzxqh9t.default/zotero/storage/3JA6932C/Ji et al. - 2013 - Differential principal component analysis of ChIP-.pdf:application/pdf;Full Text PDF:/Users/ijoseph/Library/Application Support/Zotero/Profiles/gtzxqh9t.default/zotero/storage/RB4B9BUG/Ji et al. - 2013 - Differential principal component analysis of ChIP-.pdf:application/pdf;Snapshot:/Users/ijoseph/Library/Application Support/Zotero/Profiles/gtzxqh9t.default/zotero/storage/5EIQ4R55/6789.html:text/html;Snapshot:/Users/ijoseph/Library/Application Support/Zotero/Profiles/gtzxqh9t.default/zotero/storage/Q9EM5ICD/6789.html:text/html},
