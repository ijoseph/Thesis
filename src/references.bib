  pages = {3987--3991},
  keywords = {Alkylating agents,alkylator,Brain/central nervous system cancers,chemoresistance,Clinical drug resistance,DNA damage; DNA repair; and mutagenesis,Glioma,mismatch repair; protein kinase},
  file = {Full Text PDF:/Users/ijoseph/Library/Application Support/Zotero/Profiles/gtzxqh9t.default/zotero/storage/B3FMWAH5/Hunter et al. - 2006 - A Hypermutation Phenotype and Somatic MSH6 Mutatio.pdf:application/pdf;Snapshot:/Users/ijoseph/Library/Application Support/Zotero/Profiles/gtzxqh9t.default/zotero/storage/SX85P43R/3987.html:text/html},
