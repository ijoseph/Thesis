  urldate = {2014-11-06},
  date = {2012-12-20},
  pages = {e1002838},
  file = {PLoS Full Text PDF:/Users/ijoseph/Library/Application Support/Zotero/Profiles/gtzxqh9t.default/zotero/storage/ZVVS2AIR/Qiao et al. - 2012 - PERT A Method for Expression Deconvolution of Hum.pdf:application/pdf;PLoS Snapshot:/Users/ijoseph/Library/Application Support/Zotero/Profiles/gtzxqh9t.default/zotero/storage/4J85EU3B/infodoi10.1371journal.pcbi.html:text/html}
