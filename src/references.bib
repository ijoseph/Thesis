@article{hovanes_-cateninsensitive_2001,
  title = {β-Catenin–sensitive Isoforms of Lymphoid Enhancer Factor-1 Are Selectively Expressed in Colon Cancer},
  volume = {28},
  rights = {© 2001 Nature Publishing Group},
