  urldate = {2015-11-10},
  date = {2013-01-15},
  pages = {189--196},
  file = {PubMed Central Full Text PDF:/Users/ijoseph/Library/Application Support/Zotero/Profiles/gtzxqh9t.default/zotero/storage/5FC79GH7/Teschendorff et al. - 2013 - A beta-mixture quantile normalization method for c.pdf:application/pdf},
