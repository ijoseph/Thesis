  pages = {216--220},
  keywords = {cancer,Genetics and genomics},
  file = {Full Text PDF:/Users/ijoseph/Library/Application Support/Zotero/Profiles/gtzxqh9t.default/zotero/storage/U67CJZ7D/Wang et al. - 2011 - Integrative genomics identifies LMO1 as a neurobla.pdf:application/pdf;Snapshot:/Users/ijoseph/Library/Application Support/Zotero/Profiles/gtzxqh9t.default/zotero/storage/MFKUIJSA/nature09609.html:text/html}
