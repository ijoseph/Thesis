  title = {Isocitrate Dehydrogenase 1 and 2 Mutations Induce {{BCL}}-2 Dependence in Acute Myeloid Leukemia},
  volume = {21},
  rights = {© 2014 Nature Publishing Group, a division of Macmillan Publishers Limited. All Rights Reserved.},
