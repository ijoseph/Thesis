  file = {Full Text PDF:/Users/ijoseph/Library/Application Support/Zotero/Profiles/gtzxqh9t.default/zotero/storage/C2NRESRN/Witten and Tibshirani - 2009 - Extensions of Sparse Canonical Correlation Analysi.pdf:application/pdf;Snapshot:/Users/ijoseph/Library/Application Support/Zotero/Profiles/gtzxqh9t.default/zotero/storage/B4CTS5KV/1602.html:text/html},
  eprinttype = {pmid},
