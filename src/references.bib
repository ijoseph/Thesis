  date = {2008-02},
  pages = {1198--214},
  keywords = {Adult,algorithms,Cerebral Cortex,Cerebral Cortex: anatomy & histology,Cerebral Cortex: physiology,Computer Simulation,Electroencephalography,Electroencephalography: methods,Evoked Potentials; Visual,Evoked Potentials; Visual: physiology,False Positive Reactions,Humans,Image Processing; Computer-Assisted,Image Processing; Computer-Assisted: methods,Magnetic Resonance Imaging,Magnetic Resonance Imaging: methods,Male,Oxygen,Oxygen: blood,Photic Stimulation},
  file = {Liu, He - NeuroImage - 2008 - fMRI-EEG integrated cortical source imaging by use of time-variant spatial constraints:/Users/ijoseph/Library/Application Support/Zotero/Profiles/gtzxqh9t.default/zotero/storage/JMFCJMBX/Liu, He - NeuroImage - 2008 - fMRI-EEG integrated cortical source imaging by use of time-variant spatial constraints.pdf:application/pdf}
