  shortjournal = {PNAS},
  author = {Mo, Qianxing and Wang, Sijian and Seshan, Venkatraman E. and Olshen, Adam B. and Schultz, Nikolaus and Sander, Chris and Powers, R. Scott and Ladanyi, Marc and Shen, Ronglai},
  urldate = {2015-03-02},
  date = {2013-12-03},
  pages = {4245--4250},
  keywords = {multidimensional data,multivariate generalized linear model,penalized regression},
  file = {Full Text PDF:/Users/ijoseph/Library/Application Support/Zotero/Profiles/gtzxqh9t.default/zotero/storage/7BNFP34R/Mo et al. - 2013 - Pattern discovery and cancer gene identification i.pdf:application/pdf;1208949110_pnas.201208949SI.pdf:/Users/ijoseph/Library/Application Support/Zotero/Profiles/gtzxqh9t.default/zotero/storage/WAVDIJXM/1208949110_pnas.201208949SI.pdf:application/pdf;Snapshot:/Users/ijoseph/Library/Application Support/Zotero/Profiles/gtzxqh9t.default/zotero/storage/5VHPBUC9/4245.html:text/html},
