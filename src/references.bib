@article{kleihues_primary_1999,
  title = {Primary and Secondary Glioblastomas: {{From}} Concept to Clinical Diagnosis},
  volume = {1},
  issn = {1522-8517, 1523-5866},
  url = {http://neuro-oncology.oxfordjournals.org/content/1/1/44},
  doi = {10.1093/neuonc/1.1.44},
  shorttitle = {Primary and Secondary Glioblastomas},
  abstract = {Glioblastomas may develop de novo (primary glioblastomas) or through progression from low-grade or anaplastic astrocytomas (secondary glioblastomas). These subtypes of glioblastoma constitute distinct disease entities that evolve through different genetic pathways, affect patients at different ages, and are likely to differ in prognosis and response to therapy. Primary glioblastomas develop in older patients and typically show EGFR overexpression, PTEN (MMAC1) mutations, CDKN2A (p16) deletions, and less frequently, MDM2 amplification. Secondary glioblastomas develop in younger patients and often contain TP53 mutations as the earliest detectable alteration. These characteristics are derived largely from patients selected on the basis of clinical history and sequential biopsies. Currently available data are insufficient for a substitution of histologic classification and grading of astrocytic tumors by genetic typing alone. More subtypes of glioblastomas may exist with intermediate clinical and genetic profiles, a factor exemplified by the giant-cell glioblastoma that clinically and genetically occupies a hybrid position between primary (de novo) and secondary glioblastomas. Future research should aim at the identification of criteria for a combined clinical, histologic, and genetic classification of astrocytic tumors.},
  timestamp = {2015-01-03T21:32:11Z},
  langid = {english},
  number = {1},
  journaltitle = {Neuro-Oncology},
  shortjournal = {Neuro Oncol},
  author = {Kleihues, Paul and Ohgaki, Hiroko},
  urldate = {2015-01-03},
  date = {1999-01-01},
  pages = {44--51},
  file = {Full Text PDF:/Users/ijoseph/Library/Application Support/Zotero/Profiles/gtzxqh9t.default/zotero/storage/QNS244D2/Kleihues and Ohgaki - 1999 - Primary and secondary glioblastomas From concept .pdf:application/pdf;Snapshot:/Users/ijoseph/Library/Application Support/Zotero/Profiles/gtzxqh9t.default/zotero/storage/I7VBW72D/44.html:text/html},
