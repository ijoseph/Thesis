  keywords = {Kurtosis,L-moments ratio diagram,Power,Skewness},
  file = {Full Text PDF:/Users/ijoseph/Library/Application Support/Zotero/Profiles/gtzxqh9t.default/zotero/storage/D6QC7WN8/Okrah and Bravo - 2015 - Shape analysis of high-throughput transcriptomics .pdf:application/pdf;Snapshot:/Users/ijoseph/Library/Application Support/Zotero/Profiles/gtzxqh9t.default/zotero/storage/7V98VM5B/627.html:text/html},
