  file = {Full Text PDF:/Users/ijoseph/Library/Application Support/Zotero/Profiles/gtzxqh9t.default/zotero/storage/993485RA/Smyth - 2004 - Linear Models and Empirical Bayes Methods for Asse.pdf:application/pdf;Full Text PDF:/Users/ijoseph/Library/Application Support/Zotero/Profiles/gtzxqh9t.default/zotero/storage/EHADXEQB/Smyth - 2004 - Linear Models and Empirical Bayes Methods for Asse.pdf:application/pdf;Full Text PDF:/Users/ijoseph/Library/Application Support/Zotero/Profiles/gtzxqh9t.default/zotero/storage/V2Z2QUN2/Smyth - 2004 - Linear Models and Empirical Bayes Methods for Asse.pdf:application/pdf;Snapshot:/Users/ijoseph/Library/Application Support/Zotero/Profiles/gtzxqh9t.default/zotero/storage/AK5FZE7P/sagmb.2004.3.1.1027.html:text/html;Snapshot:/Users/ijoseph/Library/Application Support/Zotero/Profiles/gtzxqh9t.default/zotero/storage/P4PX4KF7/sagmb.2004.3.1.1027.html:text/html;Snapshot:/Users/ijoseph/Library/Application Support/Zotero/Profiles/gtzxqh9t.default/zotero/storage/PDN9TPSS/sagmb.2004.3.1.1027.html:text/html}
