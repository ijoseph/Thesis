@article{yosef_dynamic_2013,
  title = {Dynamic Regulatory Network Controlling {{TH17}} Cell Differentiation},
  volume = {496},
  rights = {© 2013 Nature Publishing Group, a division of Macmillan Publishers Limited. All Rights Reserved.},
