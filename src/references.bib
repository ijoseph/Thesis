@article{cho_glioblastoma-derived_2011,
  title = {Glioblastoma-{{Derived Epidermal Growth Factor Receptor Carboxyl}}-{{Terminal Deletion Mutants Are Transforming}} and {{Are Sensitive}} to {{EGFR}}-{{Directed Therapies}}},
