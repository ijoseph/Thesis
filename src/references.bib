@article{wilson_thinking_1991,
  title = {Thinking Too Much: {{Introspection}} Can Reduce the Quality of Preferences and Decisions},
  volume = {60},
  rights = {(c) 2012 APA, all rights reserved},
  issn = {1939-1315(Electronic);0022-3514(Print)},
  doi = {10.1037/0022-3514.60.2.181},
  shorttitle = {Thinking Too Much},
  abstract = {In Study 1, 49 college students' preferences for different brands of strawberry jams were compared with experts' ratings of the jams. Students who analyzed why they felt the way they did agreed less with the experts than students who did not. In Study 2, 243 college students' preferences for college courses were compared with expert opinion. Some students were asked to analyze reasons; others were asked to evaluate all attributes of all courses. Both kinds of introspection caused people to make choices that, compared with control Ss', corresponded less with expert opinion. Analyzing reasons can focus people's attention on nonoptimal criteria, causing them to base their subsequent choices on these criteria. Evaluating multiple attributes can moderate people's judgments, causing them to discriminate less between the different alternatives.},
  timestamp = {2014-02-20T03:33:34Z},
  number = {2},
  journaltitle = {Journal of Personality and Social Psychology},
  author = {Wilson, Timothy D. and Schooler, Jonathan W.},
  date = {1991},
