@article{unal_gametogenesis_2011,
  title = {Gametogenesis {{Eliminates Age}}-{{Induced Cellular Damage}} and {{Resets Life Span}} in {{Yeast}}},
  volume = {332},
  issn = {0036-8075, 1095-9203},
