  journaltitle = {Bioinformatics},
  shortjournal = {Bioinformatics},
  author = {Piro, Vitor C. and Lindner, Martin S. and Renard, Bernhard Y.},
  urldate = {2016-03-25},
  date = {2016-03-24},
  pages = {btw150},
  file = {Full Text PDF:/Users/ijoseph/Library/Application Support/Zotero/Profiles/gtzxqh9t.default/zotero/storage/G36QJWB7/Piro et al. - 2016 - DUDes a top-down taxonomic profiler for metagenom.pdf:application/pdf;Snapshot:/Users/ijoseph/Library/Application Support/Zotero/Profiles/gtzxqh9t.default/zotero/storage/C8XFNXGG/bioinformatics.btw150.html:text/html}
