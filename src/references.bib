  pages = {613--618},
  file = {Full Text PDF:/Users/ijoseph/Library/Application Support/Zotero/Profiles/gtzxqh9t.default/zotero/storage/C4RB8VZF/Zhang et al. - 2009 - Digital RNA allelotyping reveals tissue-specific a.pdf:application/pdf;Snapshot:/Users/ijoseph/Library/Application Support/Zotero/Profiles/gtzxqh9t.default/zotero/storage/GRH8UPBT/nmeth.1357.html:text/html}
