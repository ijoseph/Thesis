  date = {2008-01-04},
  pages = {e43},
  file = {Full Text PDF:/Users/ijoseph/Library/Application Support/Zotero/Profiles/gtzxqh9t.default/zotero/storage/GR2G777T/Preter et al. - 2008 - Positional gene enrichment analysis of gene sets f.pdf:application/pdf;Snapshot:/Users/ijoseph/Library/Application Support/Zotero/Profiles/gtzxqh9t.default/zotero/storage/7N94VHCG/e43.html:text/html},
