  file = {PLoS Full Text PDF:/Users/ijoseph/Library/Application Support/Zotero/Profiles/gtzxqh9t.default/zotero/storage/ABX7E5XP/Bailey et al. - 2013 - Practical Guidelines for the Comprehensive Analysi.pdf:application/pdf;PLoS Snapshot:/Users/ijoseph/Library/Application Support/Zotero/Profiles/gtzxqh9t.default/zotero/storage/P57AWFA4/infodoi10.1371journal.pcbi.html:text/html}
