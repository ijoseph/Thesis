@article{okrah_shape_2015,
  title = {Shape Analysis of High-Throughput Transcriptomics Experiment Data},
  volume = {16},
  issn = {1465-4644, 1468-4357},
  url = {http://biostatistics.oxfordjournals.org/content/16/4/627},
  doi = {10.1093/biostatistics/kxv018},
  abstract = {The recent growth of high-throughput transcriptome technology has been paralleled by the development of statistical methodologies to analyze the data they produce. Some of these newly developed methods are based on the assumption that the data observed or a transformation of the data are relatively symmetric with light tails, usually summarized by assuming a Gaussian random component. It is indeed very difficult to assess this assumption for small sample sizes. In this article, we utilize L-moments statistics as the basis of exploratory data analysis, the assessment of distributional assumptions, and the hypothesis testing of high-throughput transcriptomic data. In particular, we use L-moments ratios for assessing the shape (skewness and kurtosis) of high-throughput transcriptome data. Based on these statistics, we propose an algorithm for identifying genes with distributions that are markedly different from the majority in the data. In addition, we also illustrate the utility of this framework to characterize the robustness of distributional assumptions. We apply it to RNA-seq data and find that methods based on the simple tt-test for differential expression analysis using L-moments as weights are robust.},
  timestamp = {2015-09-17T23:48:57Z},
  langid = {english},
