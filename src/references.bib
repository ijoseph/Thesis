  urldate = {2015-03-03},
  date = {2002-01-10},
  pages = {12963--12968},
  file = {Full Text PDF:/Users/ijoseph/Library/Application Support/Zotero/Profiles/gtzxqh9t.default/zotero/storage/R2I2SG7G/Pollack et al. - 2002 - Microarray analysis reveals a major direct role of.pdf:application/pdf;Snapshot:/Users/ijoseph/Library/Application Support/Zotero/Profiles/gtzxqh9t.default/zotero/storage/7QS87X6D/12963.html:text/html},
  eprinttype = {pmid},
