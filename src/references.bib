@article{grabherr_full-length_2011,
  title = {Full-Length Transcriptome Assembly from {{RNA}}-{{Seq}} Data without a Reference Genome},
  volume = {29},
  rights = {© 2011 Nature Publishing Group, a division of Macmillan Publishers Limited. All Rights Reserved.},
