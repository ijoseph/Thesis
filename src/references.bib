  urldate = {2015-08-10},
  date = {2006},
  pages = {464--473},
  keywords = {dimensionality reduction,Principal component analysis,semi-supervised projection,supervised projection},
  file = {ACM Full Text PDF:/Users/ijoseph/Library/Application Support/Zotero/Profiles/gtzxqh9t.default/zotero/storage/2NRUJB5C/Yu et al. - 2006 - Supervised Probabilistic Principal Component Analy.pdf:application/pdf}
}

