  pages = {37--98},
  keywords = {Bayes factors,Bayesian power calculations,Bayesian statistics,BIC criterion,Candidate gene association studies,Case–control studies,Experimental design,Family-based association studies,Frequentist statistics,Genome scans,Genome-wide association studies (GWAS),ldDesign R package,Linkage,Linkage disequilibrium,Posterior probabilities,Power calculations,Prior elicitation,Quantitative trait loci (QTL),Quantitative traits,Replication and validation,Robust detection of effects,SDT test,S-TDT test,TDT test},
