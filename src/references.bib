  urldate = {2014-07-25},
  date = {2014-05-20},
  pages = {7361--7366},
  keywords = {cell-free nucleic acids,Genomics,noninvasive diagnostics},
  file = {Full Text PDF:/Users/ijoseph/Library/Application Support/Zotero/Profiles/gtzxqh9t.default/zotero/storage/SNTHPHAV/Koh et al. - 2014 - Noninvasive in vivo monitoring of tissue-specific .pdf:application/pdf;Snapshot:/Users/ijoseph/Library/Application Support/Zotero/Profiles/gtzxqh9t.default/zotero/storage/GS5Z9ZNJ/7361.html:text/html},
