  date = {2015-06},
  pages = {473--482},
  file = {ScienceDirect Full Text PDF:/Users/ijoseph/Library/Application Support/Zotero/Profiles/gtzxqh9t.default/zotero/storage/D4QB475A/Kim et al. - 2015 - Classification of usual interstitial pneumonia in .pdf:application/pdf;ScienceDirect Snapshot:/Users/ijoseph/Library/Application Support/Zotero/Profiles/gtzxqh9t.default/zotero/storage/A2BCPD76/S221326001500140X.html:text/html}
}

