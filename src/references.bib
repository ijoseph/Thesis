  urldate = {2015-04-10},
  date = {2013-04},
  pages = {361--365},
  keywords = {APHIDICOLIN,DNA,GENE mapping,HUMAN cell membranes,MICE as laboratory animals,NUCLEOTIDE sequence,STREPTAVIDIN},
