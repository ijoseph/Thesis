  pages = {330--337},
  keywords = {cancer,genetics,Genomics,Health and medicine},
  file = {Full Text PDF:/Users/ijoseph/Library/Application Support/Zotero/Profiles/gtzxqh9t.default/zotero/storage/6FRTVU76/Network - 2012 - Comprehensive molecular characterization of human .pdf:application/pdf;Snapshot:/Users/ijoseph/Library/Application Support/Zotero/Profiles/gtzxqh9t.default/zotero/storage/KAGHAKI7/nature11252.html:text/html}
}

