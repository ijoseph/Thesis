  number = {7153},
  journaltitle = {Nature},
  author = {a Tomlins, Scott and Laxman, Bharathi and Dhanasekaran, Saravana M and Helgeson, Beth E and Cao, Xuhong and Morris, David S and Menon, Anjana and Jing, Xiaojun and Cao, Qi and Han, Bo and Yu, Jindan and Wang, Lei and Montie, James E and a Rubin, Mark and Pienta, Kenneth J and Roulston, Diane and Shah, Rajal B and Varambally, Sooryanarayana and Mehra, Rohit and Chinnaiyan, Arul M},
  date = {2007-08},
  pages = {595--9},
  keywords = {Animals,Cell Line; Tumor,Chromosome Aberrations,DNA-Binding Proteins,DNA-Binding Proteins: genetics,Humans,Male,Mice,Oncogene Proteins; Fusion,Oncogene Proteins; Fusion: genetics,Oncogenes,Oncogenes: genetics,Polymerase Chain Reaction,Prostatic Neoplasms,Prostatic Neoplasms: genetics,Prostatic Neoplasms: pathology,Proto-Oncogene Protein c-ets-1,Proto-Oncogene Protein c-ets-1: genetics,Transcription Factors,Transcription Factors: genetics},
