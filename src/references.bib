  urldate = {2015-03-05},
  date = {2013-04-15},
  pages = {1083--1085},
  file = {Full Text PDF:/Users/ijoseph/Library/Application Support/Zotero/Profiles/gtzxqh9t.default/zotero/storage/5DQU3Q2F/Gong and Szustakowski - 2013 - DeconRNASeq a statistical framework for deconvolu.pdf:application/pdf;Snapshot:/Users/ijoseph/Library/Application Support/Zotero/Profiles/gtzxqh9t.default/zotero/storage/C6XCPPSK/1083.html:text/html},
